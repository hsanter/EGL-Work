Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "The", " ", "next", " ", "three", "  ", "functions", " ", "implement", " ",
     "a", " ", "direct", " ", "sum", " ", "of", " ", "matrices"}], " ", 
   "*)"}], "\n", "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"AppendZeroes", "[", 
     RowBox[{"M_", ",", "n_"}], "]"}], ":=", 
    RowBox[{"Map", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Join", "[", 
        RowBox[{"#", ",", 
         RowBox[{"Table", "[", 
          RowBox[{"0", ",", 
           RowBox[{"{", "n", "}"}]}], "]"}]}], "]"}], "&"}], ",", "M"}], 
     "]"}]}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"AppendZeroes", "::", "usage"}], " ", "=", " ", 
     "\"\<AppendZeroes[M,n] takes a list of lists M and appends n zeros to \
each list in M.  If M is a matrix, this has the effect of adding n columns of \
zeros at the end\>\""}], ";"}], "\n", "\n", 
   RowBox[{
    RowBox[{"PrependZeroes", "[", 
     RowBox[{"M_", ",", "n_"}], "]"}], ":=", 
    RowBox[{"Map", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{"0", ",", 
           RowBox[{"{", "n", "}"}]}], "]"}], ",", "#"}], "]"}], "&"}], ",", 
      "M"}], "]"}]}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"AppendZeroes", "::", "usage"}], " ", "=", " ", 
     "\"\<PrependZeroes[M,n] takes a list of lists M and adjoins n zeros to \
the beginning of each list in M.  If M is a matri, this has the effect of \
adding n columns of zeros at the beginning\>\""}], ";"}], "\n", "\n", 
   RowBox[{
    RowBox[{"DirectSum", "[", 
     RowBox[{"A_", ",", "B_"}], "]"}], ":=", 
    RowBox[{"Join", "[", 
     RowBox[{
      RowBox[{"AppendZeroes", "[", 
       RowBox[{"A", ",", 
        RowBox[{"Length", "[", "B", "]"}]}], "]"}], ",", 
      RowBox[{"PrependZeroes", "[", 
       RowBox[{"B", ",", 
        RowBox[{"Length", "[", "A", "]"}]}], "]"}]}], "]"}]}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"DirectSum", "::", "usage"}], "=", 
     "\"\<DirectSum[A,B] returns the direct sum of the square matrices A and \
B.\>\""}], ";"}]}]}]], "Code",
